{
  "$help": "https://aka.ms/terminal-documentation",
  "$schema": "https://aka.ms/terminal-profiles-schema",
  "actions": [
    {
      "command": "paste",
      "keys": "ctrl+v"
    },
    {
      "command": {
        "action": "copy",
        "singleLine": false
      },
      "keys": "ctrl+c"
    },
    {
      "command": "find",
      "keys": "ctrl+shift+f"
    },
    {
      "command": {
        "action": "splitPane",
        "split": "auto",
        "splitMode": "duplicate"
      },
      "keys": "alt+shift+d"
    }
  ],
  "alwaysShowNotificationIcon": true,
  "copyFormatting": ["html", "rtf"],
  "copyOnSelect": false,
  "defaultProfile": "{5ae354e3-6d8b-4559-85cd-ad5b4afb853c}",
  "initialCols": 132,
  "initialRows": 32,
  "launchMode": "default",
  "centerOnLaunch": true,
  "experimental.rendering.software": true,
  "profiles": {
    "defaults": {
      "bellStyle": "audible",
      "colorScheme": "AuraDarkTheme",
      "startingDirectory": "%USERPROFILE%",
      "useAcrylic": true
    },
    "list": [
      {
        "name": "Git Bash (Windows)",
        "guid": "{}",
        "commandline": "C:\\Program Files\\Git\\bin\\bash.exe -li",
        "icon": "C:\\Program Files\\Git\\git-for-windows.ico",
